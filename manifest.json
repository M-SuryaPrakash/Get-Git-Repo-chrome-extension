{
  "name": "Get Git Repo",
  "description": "A simple extension to download github repositries (or) specific subfolders (or) only specific files.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/logo_16.png",
      "32": "/images/logo_32.png",
      "48": "/images/logo_64.png",
      "128": "/images/logo_128.png"
    }
  },
  "icons": {
    "16": "/images/logo_16.png",
    "32": "/images/logo_32.png",
    "48": "/images/logo_64.png",
    "128": "/images/logo_128.png"
  }

}
